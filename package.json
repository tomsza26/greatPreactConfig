{
  "name": "greatpreactconfig",
  "version": "1.1.0",
  "description": "Great, minimalistic template for any frontend project.",
  "author": "tomsza26",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tomsza26/greatPreactConfig/issues"
  },
  "homepage": "https://github.com/tomsza26/greatPreactConfig#readme",
  "browserslist": [
    "last 2 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomsza26/greatPreactConfig.git"
  },
  "main": "index.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.dev.ts",
    "build": "npx browserslist@latest --update-db && cross-env NODE_ENV=production webpack --config webpack/webpack.prod.ts --color --progress",
    "lint": "eslint --fix .",
    "lint:css": "stylelint src/**/*.ts",
    "pre-commit": "prettier --write . && tsc --noEmit && npm run lint:css && npm run lint && git add -A .",
    "prepare": "husky install"
  },
  "dependencies": {
    "goober": "2.1.12",
    "history": "5.3.0",
    "preact": "10.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.5",
    "react-router": "6.3.0",
    "redux": "4.2.1",
    "redux-thunk": "2.4.2",
    "reselect": "4.1.7"
  },
  "devDependencies": {
    "@swc/core": "1.3.35",
    "@types/circular-dependency-plugin": "5.0.5",
    "@types/eslint": "8.21.0",
    "@types/node": "18.13.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.10",
    "@types/react-router": "5.1.20",
    "@types/webpack": "5.28.0",
    "@types/webpack-bundle-analyzer": "4.6.0",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "circular-dependency-plugin": "5.2.2",
    "cross-env": "7.0.3",
    "eslint": "8.34.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "html-loader": "4.2.0",
